# User Agent Configuration Template
# Copy this to .env and configure for each user

# Agent Identity
AGENT_NAME=username-agent
USERNAME=username
AGENT_DOMAIN=username.karmacadabra.ultravioletadao.xyz
PORT=9000

# Blockchain Configuration
RPC_URL_FUJI=https://avalanche-fuji-c-chain-rpc.publicnode.com
CHAIN_ID=43113

# Contract Addresses (from main .env)
IDENTITY_REGISTRY=0xYourIdentityRegistryAddress
REPUTATION_REGISTRY=0xYourReputationRegistryAddress
VALIDATION_REGISTRY=0xYourValidationRegistryAddress
GLUE_TOKEN_ADDRESS=0xYourGLUETokenAddress

# Facilitator
FACILITATOR_URL=http://localhost:8080

# Agent Wallet (unique per user)
PRIVATE_KEY=0xYourPrivateKeyHere

# Agent Data Paths
AGENT_CARD_PATH=agent-card.json
PROFILE_PATH=profile.json
